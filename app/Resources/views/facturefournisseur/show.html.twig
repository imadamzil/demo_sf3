{% extends 'base.html.twig' %}

{% block content %}
    <h1>Facturefournisseur</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ facturefournisseur.id }}</td>
            </tr>
            <tr>
                <th>Achatht</th>
                <td>{{ facturefournisseur.achatHT }}</td>
            </tr>
            <tr>
                <th>Nbjours</th>
                <td>{{ facturefournisseur.nbjours }}</td>
            </tr>
            <tr>
                <th>Mois</th>
                <td>{{ facturefournisseur.mois }}</td>
            </tr>
            <tr>
                <th>Year</th>
                <td>{{ facturefournisseur.year }}</td>
            </tr>
            <tr>
                <th>Achatttc</th>
                <td>{{ facturefournisseur.achatTTC }}</td>
            </tr>
            <tr>
                <th>Taxe</th>
                <td>{{ facturefournisseur.taxe }}</td>
            </tr>
            <tr>
                <th>Facturefournisseur</th>
                <td>{{ facturefournisseur.factureFournisseur }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{{ facturefournisseur.etat }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{% if facturefournisseur.date %}{{ facturefournisseur.date|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Documentname</th>
                <td>{{ facturefournisseur.documentName }}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{% if facturefournisseur.updatedAt %}{{ facturefournisseur.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{% if facturefournisseur.createdAt %}{{ facturefournisseur.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('facturefournisseur_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('facturefournisseur_edit', { 'id': facturefournisseur.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
